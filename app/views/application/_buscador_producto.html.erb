<div class="modal fade" id="buscador_producto" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="modal-items-title">Buscador de Productos</h5>
          <button type="button" class="close" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">&times;</span>
          </button>
      </div>
      <div class="modal-body-items modal-body" id="modal-body-items">
        <div class="form-group">
          <label for="buscador">Ingresa el nombre del producto</label>
         <!-- <input type="text" class="form-control" data-tipo = " <%= @tipo %> " data-model = "<%= @model.try(:id) %>" id="input_buscador_producto" placeholder="Nombre Producto..">-->
          <input type="text" class="form-control" data-tipo="<%= @tipo %>" data-model="<%= @model.try(:id) %>" id="input_buscador_producto" placeholder="Nombre Producto..">

      </div>
      <div class="form-group">
        <label for="buscador">Ingresa una Cantidad</label>
        <input type="number" class="form-control" id="cantidad_producto" placeholder="Ingresa la cantidad.." value="1">
      </div>
      <table class="table table-dark" id="tabla_buscador">
        <thead>
          <tr>
          <th>Nombre</th>
          <th>Existencia</th>
          <th>Agregar</th>
          </tr>
        </thead>
        <tbody>
        </tbody>
      </table>
    </div>
    <div class="modal-footer" id="modal-footer-items">
      <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
    </div>
  </div>
  </div>
</div>
 <script src="vendor/jquery/jquery.min.js"></script>
